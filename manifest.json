{
  "manifest_version": 2,

  "name": "Speedy SERP",
  "description": "A semi auto solution to SERP checking",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions":[
    "tabs", "https://*/*", "background", "http://*/*", "storage"
    ], 
  "commands": {
          "_execute_page_action": {
            "suggested_key": {
              "default": "Ctrl+Shift+R",
              "mac": "Command+Shift+R"
            },
            "description": "Run page action"
  },
  "content_scripts":[
    {
     "matches": ["<all_urls>"],
     "js": ["jquery-1.10.1.min.js","contentscript.js"]
     
    }
  ]
}
