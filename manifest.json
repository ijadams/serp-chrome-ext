{
  "manifest_version": 2,

  "name": "Quik SERP 2000",
  "description": "A semi automatic solution to finding keyword rankings!",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions":[
    "tabs", "https://*/*", "background", "storage", "http://*/*"
    ],
  "content_scripts":[
    {
     "matches": ["<all_urls>"],
     "js": ["jquery-1.10.1.min.js","contentscript.js"]
     
    }
  ]
}