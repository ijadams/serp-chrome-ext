{
  "manifest_version": 2,

  "name": "Programming Mission 2",
  "description": "My solution to hackthissite.org programming mission 2",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions":[
    "tabs", "https://*/*", "background", "http://*/*", "storage"
    ],
  "content_scripts":[
    {
     "matches": ["<all_urls>"],
     "js": ["jquery-1.10.1.min.js","contentscript.js"]
     
    }
  ]
}